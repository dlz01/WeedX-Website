<!DOCTYPE html>
<html>
<head>
  <title>Timestamp Dropdown</title>
  <link rel="stylesheet" type="text/css" href="style.css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">
  <script src="https://code.jquery.com/jquery-3.7.0.min.js" integrity="sha256-2Pmvv0kuTBOenSvLm6bvfBSSHrUJ+3A7x6P5Ebd07/g=" crossorigin="anonymous"></script>
</head>
<body>
  <div id="container">
    <div id="upload-panel">
      <h2>Upload .dat File</h2>
      <form method='post' action='upload' enctype="multipart/form-data">
        <input type='file' name='fileUploaded' accept=".dat">
        <input type='submit'>
      </form>
      <div id="map">
        <iframe
          style="border:0"
          loading="lazy"
          allow="fullscreen"
          referrerpolicy="no-referrer-when-downgrade"
          src="https://www.google.com/maps/embed/v1/place?key=AIzaSyDhsI9-_E4GH3Z0O9iwHTnEsFYp06glOmk
            &q=<%= latlon[0] %>, <%= latlon[1] %>&center=<%= latlon[0] %>, <%= latlon[1] %>&zoom=12">
        </iframe>
      </div>
    </div>
    <div id="content-panel">
      <select id="fileDropdown">
        <option>Select a file</option>
        <!-- <% var fileToTime = fileToTime; %> -->
        <% var test = "test"; %>
        <% for(var i=0; i < fileToTime.size; i++) { %>
          <option value="<%= Array.from(fileToTime.keys())[i] %>"><%= Array.from(fileToTime.keys())[i] %></option>
        <% } %>
      </select>
      <!-- <h2>Select Time</h2> -->
      <select id="timeDropdown">
        <option>Select a time</option>
        <!-- <% for(var i=0; i < fileToTime.size.length; i++) { %>
          <option value="<%= Array.from(fileToTime.keys())[i] %>"><%= Array.from(fileToTime.keys())[i] %></option>
        <% } %> -->
      </select>
      <div id="timestampInfo">
        <% for (i = 0; i < contents.length; i++) { %>
          <p><%= contents[i] %></p>
        <% } %>
      </div>
      <div class="chart">
        <canvas class="my-4 w-100 chartjs-render-monitor" id="myChart" width="2150" height="1000" style="display: block; height: 453px; width: 1075px;"></canvas>
      </div>
    </div>
  </div>
  
  <script src="https://code.jquery.com/jquery-3.7.0.min.js" integrity="sha256-2Pmvv0kuTBOenSvLm6bvfBSSHrUJ+3A7x6P5Ebd07/g=" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/feather-icons@4.28.0/dist/feather.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-Fy6S3B9q64WdZWQUiU+q4/2Lc9npb8tCaSX9FK7E8HnRr0Jz8D6OP9dO5Vg3Q9ct" crossorigin="anonymous"></script>
  <script src="script.js"></script>
  <script defer>
    var fileToTime = <%- JSON.stringify(Object.fromEntries(fileToTime)) %>;
    
    function populateDropdown(selector, values) {
      var dropdown = $(selector);
      dropdown.empty();
      $.each(values, function(index, value) {
        dropdown.append($('<option></option>').attr('value', value).text(value));
      });
    }
    $(document).on('change','#fileDropdown', function()
    {
        console.log("here");
    });
    // console.log(<%=fileToTime%>);
    $('#fileDropdown').on('change', function(event) {
      console.log($('#fileDropdown').val());
      console.log(Object.entries(fileToTime));
      // populateDropdown('#timeDropdown', fileToTime[$('#fileDropdown').val()]);
    })
  </script>
  <!-- <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDhsI9-_E4GH3Z0O9iwHTnEsFYp06glOmk&callback=initMap">
  </script> -->
  <script>
    function plotChart(labels, data) {
	'use strict'
  
	feather.replace()
  
	// Graphs
	var ctx = document.getElementById('myChart')
	// eslint-disable-next-line no-unused-vars
	var myChart = new Chart(ctx, {
	  type: 'line',
	  data: {
		labels: labels,
		datasets: [{
		  data: data,
		  lineTension: 0,
		  backgroundColor: 'transparent',
		  borderColor: '#007bff',
		  borderWidth: 4,
		  pointBackgroundColor: '#007bff'
		}]
	  },
	  options: {
		scales: {
		  yAxes: [{
			ticks: {
			  beginAtZero: false
			}
		  }]
		},
		legend: {
		  display: false
		}
	  }
	})
  }
  let d = <%-JSON.stringify(data) %>;
  let l = <%-JSON.stringify(labels) %>;
  <%= func %>
  </script>
</body>
</html>

